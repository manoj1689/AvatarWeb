{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/signIn",
      "destination": "https://smart-grader-app-ts.vercel.app/signIn",
      "permanent": true
    }
  ]
}
